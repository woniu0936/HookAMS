<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".HookDemoApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.HookAMS"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ams.HookActivity"
            android:exported="true" />
        <activity
            android:name=".activity.HookStartActivity"
            android:exported="true" />
        <activity
            android:name=".activity.hook01.HookStartActivity01"
            android:exported="true" />
        <activity
            android:name=".activity.hook02.HookStartActivity02"
            android:exported="true" />
        <activity
            android:name=".activity.hook03.HookStartActivity03"
            android:exported="true" />
        <activity
            android:name=".activity.hook04.HookStartActivity04"
            android:exported="true" />
        <activity
            android:name=".replace.StubActivity"
            android:exported="true" />
    </application>

</manifest>